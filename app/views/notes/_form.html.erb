
<%= form_for @note do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :priority %> 
  <%= f.select :priority, [1,2,3,4,5] %>

  <%= f. label :complete %>
  <%= f.select(:complete, options_for_select([['Incomplete', false], ['Complete', true]], @note.complete)) %>

  <%= f.submit %>
<% end %>